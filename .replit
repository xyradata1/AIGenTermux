modules = ["python-3.11"]
[agent]
expertMode = true
integrations = ["python_openai_ai_integrations:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["unzip"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Run AIGenTermux"

[[workflows.workflow]]
name = "Run AIGenTermux"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python -m src.aigentermux.main"

[workflows.workflow.metadata]
outputType = "console"
